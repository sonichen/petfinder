<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cyj.mapper.NoticeMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.cyj.pojo.Notice">
                    <id column="id" property="id"/>
                    <result column="userId" property="userId"/>
                    <result column="categoriesId" property="categoriesId"/>
                    <result column="title" property="title"/>
                    <result column="content" property="content"/>
                    <result column="create_time" property="createTime"/>
                    <result column="update_time" property="updateTime"/>
                    <result column="click" property="click"/>
                    <result column="star" property="star"/>
                    <result column="version" property="version"></result>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, userId, categoriesId, title, content, create_time, update_time, click, star,version
        </sql>

</mapper>